<view class="assessment-content">
 <van-cell-group>
  <van-cell title="考核项" center value="{{ value }}" :border="{{ false }}" />
  <van-field
    value="{{ message }}"
    label="留言"
    type="textarea"
    placeholder="请输入留言"
    autosize="{{ params }}"
  />
  <van-uploader file-list="{{ fileList }}" bind:after-read="afterRead" />
 </van-cell-group>

</view>
