.upload {
  padding: 0 12px;
  box-sizing: border-box;
  width: 100%;
}
.upload .title {
  height: 50px;
  line-height: 50px;
  color: #333333;
  font-size: 14px;
}
.footer {
  position: fixed;
  width: 100%;
  height: 60px;
  padding: 8px 12px;
  box-sizing: border-box;
  bottom: 0;
  background: #FFFFFF;
  left: 0;
}
.footer .btn {
  width: 100%;
  border-radius: 6px;
}
